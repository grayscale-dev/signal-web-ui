{
  "name": "@signal-web-ui/core",
  "version": "0.1.10",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/signal-web-ui/signal-web-ui.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/signal-web-ui/signal-web-ui",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/signal-web-ui/signal-web-ui.js",
  "jsdelivr": "./dist/signal-web-ui/signal-web-ui.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./custom-elements": {
      "types": "./dist/custom-elements/index.d.ts",
      "import": "./dist/custom-elements/index.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts"
    },
    "./dist/custom-elements/*": {
      "types": "./dist/custom-elements/*.d.ts",
      "import": "./dist/custom-elements/*.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "docs/",
    "loader/",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "stencil build --docs",
    "dev": "stencil build --dev --watch --serve",
    "start": "stencil build --dev --watch --serve",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "stencil build --docs && vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@signal-web-ui/tsconfig": "workspace:*",
    "@stencil/angular-output-target": "^1.3.0",
    "@stencil/core": "^4.20.0",
    "@stencil/react-output-target": "^1.4.0",
    "@stencil/vue-output-target": "^0.13.0",
    "@types/node": "^22.19.7",
    "@vitest/coverage-v8": "^2.1.4",
    "jsdom": "^24.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-runner": "^29.7.0",
    "vitest": "^2.1.4"
  }
}
