{
  "name": "@signal-web-ui/angular",
  "version": "0.1.4",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/signal-web-ui/signal-web-ui.git",
    "directory": "packages/angular"
  },
  "homepage": "https://github.com/signal-web-ui/signal-web-ui",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "pnpm run typecheck"
  },
  "dependencies": {
    "@signal-web-ui/core": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "@angular/core": ">=12.0.0",
    "rxjs": ">=7.0.0"
  },
  "devDependencies": {
    "@signal-web-ui/tsconfig": "workspace:*",
    "@types/node": "^22.19.7",
    "typescript": "^5.6.3"
  }
}
